<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="format-detection" content="telephone=no">
    <title>后台管理系统</title>
    <link rel="stylesheet" href="/assets/vendor/layui/css/layui.css"/>
    <link rel="stylesheet" href="/assets/model/common/css/style.css"/>
    <link rel="stylesheet" href="/assets/model/common/css/common.css"/>
    <script type="text/javascript" src="/assets/vendor/layui/layui.js"></script>
    <!--[if lt IE 9]>
    <script src="/assets/model/common/js/html5shiv.min.js"></script>
    <script src="/assets/model/common/js/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<div class="table_wrap" style="padding: 0 15px;">
    <table id="idTest" lay-filter="demo" class="layui-table"></table>
</div>

<script>
    
    // console.log(where);
    var layui_obj = layui.config({
        //version: true,
        base: '/assets/model/'
    }).extend({
        common: 'common/js/common',
    }).use(['common','jquery','table'], function(){
        var table   = layui.table;
        var $ = layui.jquery;
        var common = layui.common;

        var url = 'vip_statistic/sellStatisticMonthDetail';

        var where = common.urlParam;

        table.render({
            elem: '#idTest'
            ,url: common.getUrl(url)
            ,headers:common.getHeader()
            ,page: false //开启分页
            ,minWidth:1000
            ,autoWith:true
            ,where:where
            ,method:'post'
            ,cols: [[ //表头

                 {field:'kpi_date_str', minWidth:90, sort: true, title:'月份'}
                ,{field:'product_id_str', minWidth:300, title:'游戏产品'}
                ,{field:'sum_kpi_value', minWidth:110, title:'月KPI业绩'}
                ,{field:'order_sum', minWidth:120, title:'营销总业绩'}
                ,{field:'order_kip_present', minWidth:110, title:'KPI完成率(%)'}
                ,{field:'big_order_count', minWidth:100, title:'大单数量'}
                ,{field:'big_order_sum', minWidth:100, title:'大单金额'}
                ,{field:'big_order_sum_present', minWidth:100, title:'大单占比(%)'}
                ,{field:'maintenance_present_str', minWidth:100, title:'活跃覆盖率',alt:'30天累计充值达标'}
                ,{field:'remark_present_str', minWidth:100, title:'月联系率',}
                ,{field:'washed_away_present_str', minWidth:100, title:'充值流失率',}
                // ,{field:'login_lost_present_str', minWidth:100, title:'登录流失率',}
            ]]
            ,done:(a,b,c)=>{
                // console.log(a,b,c,where);
            }

        });
    });
</script>
</body>
</html>