.help-block{
    color: #999;
    font-size: 12px;
}
.label-required-next:before {
    content:"*";
    color:red;
    position: absolute;
    top:5px;
    left:15px;
}
.lxx_title{
    line-height: 36px;
    border-left: 5px solid #009E94;
    margin: 15px 0 5px;
    padding: 0 10px;
    background-color: #f2f2f2;
}
.common_tip_div{
    padding-left: 10px;
    margin: 0px 20px 20px 20px;
    border: solid 1px #EEEEEE;
    /*width: 90%;*/
    /*min-width: 500px;*/
}
.common_tip_div li{
    line-height: 20px;
}
label.required_after:after {   /* 给红色的必填项打红色星号（文字后）如果打前面用before*/
    content: ' *';
    color: red;
}
label.required_before:before {   /* 给红色的必填项打红色星号（文字后）如果打前面用before*/
    content: ' *';
    color: red;
}
.search-float-right{
     min-height: 40px;
     padding-right: 15px;
 }
.search-float-right .layui-btn-group{
    float: right;
}
.mobile_parent{
    position: relative;
}
.mobile_child{
    position: absolute;
    top: 0;
    left: 0;
    display: inline-block;
    width: 120%;
    height: 100%;
    background: #fff;
}
body{
    background: #fff;
}
.clear{clear:both;}